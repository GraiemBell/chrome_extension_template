  {
    "manifest_version": 3,
    "name": "NEXT-CHROME-STARTER",
    "short_name": "N-C-S",
    "version": "0.0.1",
    "description": "Next.js Chrome Extension Starter, Demo Example by ibnzUK. https://github.com/ibnzuk/njs-es",
    "icons": {
      "16": "/icons/favicon-16x16.png",
      "32": "/icons/favicon-32x32.png",
      "192": "/icons/android-chrome-192x192.png"
    },
    "permissions": [],
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["inject.js"]
      }
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html"
    }
  }